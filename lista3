// Faça uma função que produza o inverso de uma string
#include <stdio.h>
#include <string.h>
char inverso(char *p){
   if(*p == '\0'){
       return 0; //  retorna nada
   }
   inverso(p+1); // desempilha
   
    printf("%c",*p);
    
}

int main() {
   char vetor[6] = "roma";
   inverso(vetor);
    return 0;
}
